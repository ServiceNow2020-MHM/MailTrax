        <description>Use the MyDateTimeAjax Script Include to retrieve the current date/time when form is loaded.</description>
        <field>u_action_requested</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>MailTrax Populate Current Date</name>
        <order/>
        <script><![CDATA[function onLoad() {
// Instantiate the MyDateTimeAjax Script Include
   var getNow = new GlideAjax('MyDateTimeAjax'); 

   getNow.addParam('sysparm_name', 'nowDateTime');
   getNow.getXML(function () 
{ 
g_form.setValue('u_log_date', getNow.getAnswer()); 
}); 
}]]></script>
